train_net: "models/AlexNet/train.prototxt"
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 80000
display: 20
average_loss: 100
momentum: 0.9
weight_decay: 0.0005
# We disable standard alex solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "alexnet_fast_rcnn"
#debug_info: true
